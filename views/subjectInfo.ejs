<% include partials/header %>
<link rel="stylesheet" href="/subjectInfo.css">
<h1><em><strong id="name"> <%= data["name"]  %> </strong></em></h1>  
<div>
    <div class="row">

    </div>
    <div class="col-md-4">
        <div class="thumbnail">
            <% if(data["lecture"] === "true"){ %>
                <h2><em><strong>LECTURES : </strong></em> </h2>
                    <h3>ATTENDED LECTURES : <%= data["attendedLecture"] %> </h3>
                    <h3>TOTAL LECTURES : <%= data["totalLecture"] %> </h3>
                    <% if(data["totalLecture"] !== 0){ %>
                        <% var lectureP = (data["attendedLecture"]/data["totalLecture"] * 100).toFixed(0) %>
                        <h3>LECTURE % : <%= lectureP %> </h3>
                        <div class="circleLec progress-circle p<%= lectureP %>">
                            <span class="lectureP"><%= lectureP %>  </span>
                            <div class="left-half-clipper">
                               <div class="first50-bar"></div>
                               <div class="value-bar"></div>
                            </div>
                         </div> 
                    <% } %>
                    <a class="btn btn-success" href="/user/<%= userId %>/subjectInfo/<%= data['_id'] %>/lectureAttended">ATTENDED</a> 
                    <a class="btn btn-warning" href="/user/<%= userId %>/subjectInfo/<%= data['_id'] %>/lectureMissed">MISSED</a> 
            <% } else{ %>
                <h2>NO LECTURES FOR <em><strong><%= data["name"] %></strong></em> </h2>
                <img src="data:image/png;base64," alt="">
            <% } %>
        </div>    
    </div>
    <div class="col-md-4">
        <div class="thumbnail">
            <% if(data["tutorial"] === "true"){ %>
                <h2><em><strong>TUTORIALS : </strong></em> </h2>
                <h3>ATTENDED TUTS : <%= data["attendedTut"] %> </h3>
                <h3>TOTAL TUTS : <%= data["totalTut"] %> </h3>
                <% if(data["totalTut"] !== 0){ %>
                    <% var tutP = (data["attendedTut"]/data["totalTut"] * 100).toFixed(0) %>
                    <h3>TUTORIAL % : <%= tutP %> </h3>
                    <div class="circleTut progress-circle p<%= tutP %>">
                        <span class="tutP"><%= tutP %>  </span>
                        <div class="left-half-clipper">
                           <div class="first50-bar"></div>
                           <div class="value-bar"></div>
                        </div>
                     </div> 
                <% } %>
                <a class="btn btn-success" href="/user/<%= userId %>/subjectInfo/<%= data['_id'] %>/tutAttended">ATTENDED</a> 
                <a class="btn btn-warning" href="/user/<%= userId %>/subjectInfo/<%= data['_id'] %>/tutMissed">MISSED</a> 
            <% } else{ %>
                <h2>NO TUTS FOR <em><strong><%= data["name"] %></strong></em> </h2>
                <img src="data:image/png;base64," alt="">
            <% } %>
        </div>  
    </div>    
    <div class="col-md-4">
        <div class="thumbnail">
            <% if(data["lab"] === "true"){ %>
                <h2><em><strong> LABS : </strong></em></h2>
                <h3>ATTENDED LABS : <%= data["attendedLab"] %> </h3>
                <h3>TOTAL LABS : <%= data["totalLab"] %> </h3>
                <% if(data["totalLab"] !== 0){ %>
                    <% var labP = (data["attendedLab"]/data["totalLab"] * 100).toFixed(0) %>
                    <h3>LAB % : <%= labP %> </h3>
                    <div class="circleLab progress-circle p<%= labP %>">
                        <span class="labP"><%= labP %>  </span>
                        <div class="left-half-clipper">
                           <div class="first50-bar"></div>
                           <div class="value-bar"></div>
                        </div>
                     </div> 
                <% } %>
                <a class="btn btn-success" href="/user/<%= userId %>/subjectInfo/<%= data['_id'] %>/labAttended">ATTENDED</a> 
                <a class="btn btn-warning" href="/user/<%= userId %>/subjectInfo/<%= data['_id'] %>/labMissed">MISSED</a> 
            <% } else{ %>
                <h2>NO LABS FOR <em><strong><%= data["name"] %></strong></em>  </h2>
                <img src="data:image/png;base64," alt="">
            <% } %>
            </div>    
    </div>
    </div>
    </div>

    <script src="/subjectInfo.js"></script>
    </body>
</html>